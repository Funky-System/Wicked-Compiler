class mua

    var mua_var = 12345

    function hoi(inc)
       return "abc" + inc
    end

end

class bla extends mua

    var i = 9

    function new(k)
        this.i = 2 * k
    end

    function hoi(inc)
        var i = 123
        this.i += inc
        this.i += this.hoki()
        return this.i
    end

    function hoki()
        return 4
    end

end

function basic(i)
    return 199 + i
end

for i in [1..60*60] do
    var greet = new bla(3)
    var j = greet.mua_var
    j = greet.hoi(basic(3))
end

